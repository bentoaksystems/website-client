    "@angular/platform-server": "^6.1.9",
    "@angular/router": "^6.1.9",
    "@ng-bootstrap/ng-bootstrap": "^3.3.0",
    "@nguniversal/express-engine": "^5.0.0",
    "@nguniversal/module-map-ngfactory-loader": "^5.0.0",
    "contentful": "^4.6.1",
