    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.1",
    "reflect-metadata": "^0.1.10",
    "ts-loader": "^2.3.7",
    "ts-node": "~3.0.4",
    "tslint": "^5.3.2",
    "typescript": "^2.6.2"
  }
